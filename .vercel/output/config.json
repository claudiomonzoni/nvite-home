{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/addInvitados\\.json/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/getInvitado\\.json/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/getInvitados\\.json/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/ingresar\\.json/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/keystatic(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/registro\\.json/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/salir\\.json/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/([^/]+?)\\.json/?$",
			"dest": "_render"
		},
		{
			"src": "^/bodas/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/keystatic(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/nvitaciones/cart/?$",
			"dest": "_render"
		},
		{
			"src": "^/nvitaciones/checkout/?$",
			"dest": "_render"
		},
		{
			"src": "^/nvitaciones/success/?$",
			"dest": "_render"
		},
		{
			"src": "^/nvitaciones/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/panel/ingresar/?$",
			"dest": "_render"
		},
		{
			"src": "^/panel/lib/formSubmit/?$",
			"dest": "_render"
		},
		{
			"src": "^/panel/registro/?$",
			"dest": "_render"
		},
		{
			"src": "^/panel/?$",
			"dest": "_render"
		},
		{
			"src": "^/quince/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/robots\\.txt/?$",
			"dest": "_render"
		},
		{
			"src": "^/terminos-condiciones/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "/404.html",
			"status": 404
		}
	]
}